{
  "name": "akimutin_avtomoto",
  "homepage": "https://ampu.github.io/akimutin_avtomoto/",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "npm run editorconfig && npm run stylelint && npm run eslint",
    "editorconfig": "editorconfig-cli",
    "stylelint": "stylelint 'src/**/*.scss' --syntax scss",
    "eslint": "eslint --ext .jsx --ext .js src/"
  },
  "editorconfig-cli": [
    "*.json",
    "*.js",
    "public/*.html",
    "src/js/**/*.js",
    "src/img/**/*.svg",
    "src/**/*.scss"
  ],
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all",
      "not ie <= 11"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "classnames": "2.3.1",
    "dayjs": "1.10.5",
    "focus-trap-react": "8.6.0",
    "lodash": "4.17.21",
    "nanoid": "3.1.23",
    "normalize.css": "8.0.1",
    "prop-types": "15.7.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-redux": "7.2.4",
    "react-router-dom": "5.2.0",
    "react-scripts": "4.0.3",
    "react-yandex-maps": "4.6.0",
    "redux": "4.1.0",
    "redux-devtools-extension": "2.13.9"
  },
  "devDependencies": {
    "@htmlacademy/editorconfig-cli": "1.0.0",
    "eslint-config-htmlacademy": "4.0.2",
    "eslint-plugin-react": "7.24.0",
    "eslint-plugin-react-hooks": "4.2.0",
    "sass": "1.35.1",
    "stylelint": "13.13.1",
    "stylelint-config-htmlacademy": "0.1.10"
  }
}
